<Alloy>
	<Collection src="etablishment" />
	<Window id="moment" title="Happy Hours" tabBarHidden="true">
       <ScrollView layout="vertical" width="100%" height="90%" id="happyhourcontents" dataCollection="etablishment" dataFilter="haveHappyFilter" dataTransform="test">
			<View width="100%" height="20%" idEtablishment="{id}" titleEtablishment="{name}" onClick="goEtablishment">
				<Label text="{name}" class="name" />
				<Label text="{now}" class="happy"/>
			</View>
			<View backgroundColor="black" width="100%" height="1px" ></View>
		</ScrollView>
    </Window>
</Alloy>